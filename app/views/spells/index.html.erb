<div>
  <h1 class="font-bold text-4xl">Spells#index</h1>
  <p>Find me in app/views/spells/index.html.erb</p>

  <div id="spells" class="min-w-full divide-y divide-gray-200 space-y-5">
    <% if @spells.any? %>
      <% @spells.each do |spell| %>
        <div class="flex flex-col justify-between items-center pb-5">
          <%= render spell %>
        </div>
      <% end %>
    <% end %>
  </div>
</div>
